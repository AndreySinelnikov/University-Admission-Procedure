applicant_num = int(input())
accepted_num = int(input())
mainlist = []

for _ in range(applicant_num):
    entry = input().split(" ")
    fullname = f"{entry[0]} {entry[1]}"
    gpa = float(entry[2])
    mainlist.append([f"{gpa:.2f}", fullname])

mainlist.sort(key=lambda x: (-float(x[0]), x[1]))

print("Successful applicants:")
for elem in mainlist[0:accepted_num]:
    print(elem[1])
